import requests
import pandas as pd
import io
import seaborn as sns
import matplotlib.pyplot as plt; plt.rcdefaults()
import numpy as np
import matplotlib.pyplot as plt

Cont = requests.get('')
chunks = []
df_chunk = pd.read_csv(io.BytesIO(cont.content),header=9, error_bad_lines = False, chunksize = 100000)
for chunk in df_chunk:
            chunk = chunk[['average_sales_price','asin','UPC', 'Title', 'Manufacturer', 'Brand', 'Client Product Group', 'Category', 'Subcategory', 'Amazon Sub Category', 'sales', 'third_party_ordered_sales']]
            chunks.append(chunk)
print(chunks)
sns.regplot('average_sales_price', 'sales', data=chunk)
